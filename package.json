{
  "name": "vue-cookies-network-uikit",
  "description": "UI Kit for Cookies Network",
  "private": true,
  "version": "0.0.0",
  "author": "miirsery",
  "type": "module",
  "main": "./dist/vue-cookies-network-uikit.umd.cjs",
  "types": "./dist/vue-cookies-network-uikit.d.ts",
  "module": "./dist/vue-cookies-network-uikit.js",
  "exports": {
    ".": {
      "types": "./dist/vue-cookies-network-uikit.d.ts",
      "import": "./dist/vue-cookies-network-uikit.js",
      "require": "./dist/vue-cookies-network-uikit.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miirsery/vue-uikit.git"
  },
  "bugs": {
    "url": "https://github.com/miirsery/vue-uikit/issues"
  },
  "homepage": "https://github.com/miirsery/vue-uikit#readme",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"**/*.{vue,css,scss}\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "js-cookie": "^3.0.5",
    "maska": "^3.0.3",
    "vue": "^3.5.11",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.9.0",
    "@eslint/js": "^9.9.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-interactions": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/addon-onboarding": "^8.3.0",
    "@storybook/blocks": "^8.3.0",
    "@storybook/test": "^8.3.0",
    "@storybook/vue3": "^8.3.0",
    "@storybook/vue3-vite": "^8.3.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitejs/plugin-vue": "^5.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "storybook": "^8.3.0",
    "stylelint": "^16.8.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.0",
    "vite-plugin-static-copy": "^1.0.6",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.29"
  },
  "peerDependencies": {
    "vue": "^3.5.11"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
